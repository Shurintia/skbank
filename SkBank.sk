command /islem [<text>] [<text>]:
    trigger:
        if {skBank::%player%::kilit} is true:
            arg-1 is "yatir":
                arg-2 is "1" or "2" or "3" or "4":
                    {skBank::bakiye::%player%} < {skBank::maksBakiye::%player%}:
                        {skBank::borsa::islem::%arg-2%} + {skBank::bakiye::%player%} <= {skBank::maksBakiye::%player%}:
                            player's balance >= {skBank::borsa::islem::%arg-2%}:
                                remove {skBank::borsa::islem::%arg-2%} from player's balance
                                add {skBank::borsa::islem::%arg-2%} to {skBank::bakiye::%player%}
                                send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Islem-Basarili"" in ""SkBank/Mesajlar"", executor)%"
                            player's balance < {skBank::borsa::islem::%arg-2%}:
                                send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Bakiye-Veya-Para-Yetersiz"" in ""SkBank/Mesajlar"", executor)%"
                        else:
                            send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Banka-Dolu"" in ""SkBank/Mesajlar"", executor)%"
                    else:
                        send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Banka-Dolu"" in ""SkBank/Mesajlar"", executor)%"
            arg-1 is "cek":
                arg-2 is "1" or "2" or "3" or "4":
                    {skBank::bakiye::%player%} >= {skBank::borsa::islem::%arg-2%}:
                        remove {skBank::borsa::islem::%arg-2%} from {skBank::bakiye::%player%}
                        add {skBank::borsa::islem::%arg-2%} to player's balance
                        send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Islem-Basarili"" in ""SkBank/Mesajlar"", executor)%"
                    {skBank::bakiye::%player%} < {skBank::borsa::islem::%arg-2%}:
                        send "%{prefix}% %placeholder(yaml value ""Islem-Mesajlari.Bakiye-Veya-Para-Yetersiz"" in ""SkBank/Mesajlar"", executor)%"
        else:
            send "%{prefix}% %placeholder(yaml value ""Durum-Mesajlari.Giris-Yapiniz"" in ""SkBank/Mesajlar"", executor)%"
